public class Caisses {

    /**
     * getNom: retourne le nom de la caisse à partir de son numéro.
     * 
     * {talendTypes} String
     * 
     * {Category} Gestion des caisses
     * 
     * {param} int("Num_Caisse") input: Numéro de la caisse.
     * 
     * {example} Caisses.getNom(1) # "CAVOM".
     */
    public static String getNom(int Num_Caisse) {
    String nomCaisse = "";
        if (Num_Caisse == 1)
            nomCaisse = "CAVOM"; 
        else if (Num_Caisse == 2)
        	nomCaisse = "CAVEC";
        else if (Num_Caisse == 3)
        	nomCaisse = "CIPAV";
        else if (Num_Caisse == 4)
        	nomCaisse = "IRCEC";
        else if (Num_Caisse == 5)
        	nomCaisse = "CIPAV";
        else
        	nomCaisse = "??";
        return nomCaisse;
    }
	/**
     * getNom: retourne le nom de la caisse à partir de son numéro.
     * 
     * {talendTypes} String
     * 
     * {Category} Gestion des caisses
     * 
     * {param} String("Num_Caisse") input: Numéro de la caisse.
     * 
     * {example} Caisses.getNom("1") # "CAVOM".
     */
    public static String getNom(String Num_Caisse) {
        String nomCaisse = "";
            if ("1".equals(Num_Caisse))
                nomCaisse = "CAVOM"; 
            else if ("2".equals(Num_Caisse))
            	nomCaisse = "CAVEC";
            else if ("3".equals(Num_Caisse))
            	nomCaisse = "CIPAV";
            else if ("4".equals(Num_Caisse))
            	nomCaisse = "IRCEC";
            else if ("5".equals(Num_Caisse))
            	nomCaisse = "CIPAV";
            else
            	nomCaisse = "??";
            return nomCaisse;
        }
    /**
     * getNom: retourne le numéro de la caisse à partir de son nom ou de son abréviation.
     * 
     * {talendTypes} String
     * 
     * {Category} Gestion des caisses
     * 
     * {param} int("nomCaisse") input: Nom complet ou abréviation de la caisse.
     * 
     * {example} Caisses.getNum("CAVOM") # 1.
     * {example} Caisses.getNum("OM") # 1.
     */
    public static int getNum(String nomCaisse) {
        int numCaisse = 0;
            if ("CAVOM".equals(nomCaisse) || "OM".equals(nomCaisse))
                numCaisse = 1; 
            else if ("CAVEC".equals(nomCaisse) || "EC".equals(nomCaisse))
                numCaisse = 2;
            else if ("CIPAV".equals(nomCaisse) || "CI".equals(nomCaisse) || "IT".equals(nomCaisse))
                numCaisse = 3;
            else if ("IRCEC".equals(nomCaisse) || "IR".equals(nomCaisse))
                numCaisse = 4;
            else
            	numCaisse = -1;
            return numCaisse;
        }
    /**
     * getNom: retourne le nom abrégé de la caisse à partir de son numéro.
     * 
     * {talendTypes} String
     * 
     * {Category} Gestion des caisses
     * 
     * {param} int("Num_Caisse") input: Numéro de la caisse.
     * 
     * {example} Caisses.getAbrv(1) # "OM".
     */
    public static String getAbrv(int Num_Caisse) {
    String nomCaisse = "";
        if (Num_Caisse == 1)
            nomCaisse = "OM"; 
        else if (Num_Caisse == 2)
        	nomCaisse = "EC";
        else if (Num_Caisse == 3 || Num_Caisse == 5)
        	nomCaisse = "CI";
        else if (Num_Caisse == 4)
        	nomCaisse = "IR";
        else
        	nomCaisse = "??";
        return nomCaisse;
    }
    /**
     * getNom: retourne le nom abrégé de la caisse à partir de son numéro ou de son nom.
     * 
     * {talendTypes} String
     * 
     * {Category} Gestion des caisses
     * 
     * {param} String("idCaisse") input: Numéro ou nom complet de la caisse.
     * 
     * {example} Caisses.getAbrv("1") # "OM".
     * {example} Caisses.getAbrv("CAVOM") # "OM".
     */
    public static String getAbrv(String idCaisse) {
    String nomCaisse = "";
        if ("CAVOM".equals(idCaisse) || "1".equals(idCaisse))
        	nomCaisse = "OM"; 
        else if ("CAVEC".equals(nomCaisse) || "2".equals(idCaisse))
        	nomCaisse = "EC"; 
        else if ("CIPAV".equals(nomCaisse) || "3".equals(idCaisse) || "5".equals(idCaisse))
        	nomCaisse = "CI"; 
        else if ("IRCEC".equals(nomCaisse) || "4".equals(idCaisse))
        	nomCaisse = "IR"; 
        else
        	nomCaisse = "??";
        return nomCaisse;
    }
}
